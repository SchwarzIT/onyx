{
  "name": "@sit-onyx/icons-vue",
  "description": "Native Vue wrapper around the icons for the onyx design system",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schwarzit/onyx",
    "directory": "packages/icons-vue"
  },
  "bugs": {
    "url": "https://github.com/schwarzit/onyx/issues"
  },
  "scripts": {
    "build": "pnpm run '/type-check|build-only/'",
    "build-only": "pnpm generate && vite build && rm -rf ./src/tmp",
    "generate": "tsx ./src/index.ts",
    "type-check": "vue-tsc --noEmit --composite false"
  },
  "peerDependencies": {
    "@sit-onyx/icons": "workspace:^"
  },
  "devDependencies": {
    "tsx": "^4.7.0"
  }
}
