{
  "extends": ["@vue/tsconfig/tsconfig.dom.json", "@vue/tsconfig/tsconfig.lib.json"],
  "include": ["src/**/*"],
  "compilerOptions": {
    "rootDir": "./src",
    "baseUrl": ".",
    "outDir": "dist",
    "skipLibCheck": true,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "allowImportingTsExtensions": false
  }
}
